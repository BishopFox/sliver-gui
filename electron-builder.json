{
  "productName": "sliver",
  "directories": {
    "output": "release/"
  },
  "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!LICENSE.md",
      "!package.json",
      "!package-lock.json",
      "!src/",
      "!e2e/",
      "!hooks/",
      "!angular.json",
      "!_config.yml",
      "!karma.conf.js",
      "!tsconfig.json",
      "!tslint.json"
  ],
  "win": {
    "icon": "dist/en",
    "target": [
      "portable",
      "nsis"
    ],
    "publish": [{
      "provider": "github",
      "owner": "moloch--",
      "repo": "sliver-gui"
    }]
  },
  "mac": {
    "icon": "dist/en",
    "target": [
      "dmg"
    ],
    "publish": [{
      "provider": "github",
      "owner": "moloch--",
      "repo": "sliver-gui"
    }]
  },
  "linux": {
    "icon": "dist/en",
    "target": [
      "deb",
      "rpm",
      "AppImage"
    ],
    "publish": [{
      "provider": "github",
      "owner": "moloch--",
      "repo": "sliver-gui"
    }]
  }
}
