{
  "name": "sliver-gui",
  "version": "1.0.0",
  "description": "Cross-platform GUI for the Sliver implant framework",
  "license": "GPLv3",
  "homepage": "https://github.com/moloch--/sliver-gui",
  "author": {
    "name": "moloch",
    "email": "jdemesy@bishopfox.com"
  },
  "repository": "https://github.com/moloch--/sliver-gui",
  "main": "main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build:worker": "cd workers/worker && npm run build",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run version && npm run build:worker && npm run build -- -c dev",
    "build:prod": "npm run version && npm run build:worker && npm run build -- -c production",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:local": "npm run build:dev && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac --publish=never",
    "i18n": "ng extract-i18n --ivy --output-path ./src/locale/ && xliffmerge --profile ./src/locale/xliffmerge.json en fr ja es zh",
    "version": "ts-node -O '{\"module\": \"commonjs\"}' ./version.ts",
    "test": "ng test",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular/cli": "11.0.1",
    "@angular/common": "11.0.0",
    "@angular/compiler": "11.0.0",
    "@angular/compiler-cli": "11.0.0",
    "@angular/core": "11.0.0",
    "@angular/forms": "11.0.0",
    "@angular/language-service": "11.0.0",
    "@angular/platform-browser": "11.0.0",
    "@angular/platform-browser-dynamic": "11.0.0",
    "@angular/router": "11.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "5.2.7",
    "@types/node": "^12.11.1",
    "@types/sequelize": "^4.28.9",
    "@types/uuid": "^3.4.8",
    "chai": "4.2.0",
    "codelyzer": "^6.0.0",
    "conventional-changelog-cli": "2.0.25",
    "core-js": "3.3.3",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "^6.2.2",
    "ngx-i18nsupport": "^0.17.1",
    "npm-run-all": "4.1.5",
    "spectron": "9.0.0",
    "ts-node": "8.4.1",
    "ts-protoc-gen": "^0.12.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "wait-on": "3.3.0",
    "webdriver-manager": "12.1.7",
    "webpack-cli": "^3.3.11",
    "zone.js": "~0.10.2"
  },
  "engines": {
    "node": ">=10.9.0"
  },
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^11.0.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/localize": "^11.0.0",
    "@angular/material": "^11.0.0",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@materia-ui/ngx-monaco-editor": "^4.0.2",
    "ajv": "^6.12.0",
    "google-protobuf": "^3.11.4",
    "handlebars": "^4.7.6",
    "js-base64": "^3.6.0",
    "log4js": "^6.3.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "monaco-editor": "^0.18.1",
    "protobufjs": "^6.8.8",
    "rxjs": "^6.5.3",
    "sequelize": "^6.3.5",
    "sliver-script": "^1.0.32",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.1",
    "xterm": "^4.4.0",
    "xterm-addon-fit": "^0.4.0"
  }
}
